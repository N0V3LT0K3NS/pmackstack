{
  "rewrites": [
    { "source": "/api/(.*)", "destination": "/api/$1" },
    { "source": "/(.*)", "destination": "/index.html" }
  ],
  "routes": [
    { "handle": "filesystem" },
    { "src": "/api/.*", "dest": "https://pmackstack-production-0f8a.up.railway.app/api/$1" },
    { "src": "/.*", "dest": "/index.html" }
  ]
}
